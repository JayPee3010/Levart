<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Levart.UserInterface.OverviewPage">

     <ContentPage.ToolbarItems>
        <ToolbarItem Text="New" Clicked="OnNewPhotoClicked"/>
    </ContentPage.ToolbarItems>
    
	<ContentPage.Content>
<StackLayout>
            <ScrollView>
   <Grid BackgroundColor="Black">
    <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
            <RowDefinition Height="75"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>         
        </Grid.ColumnDefinitions>
        <Image x:Name="Image1" Grid.Row="0" Grid.Column="0" BackgroundColor="White" />
        <Button Grid.Row="0" Grid.Column="1" BackgroundColor="White" />
        <Button Grid.Row="0" Grid.Column="2" BackgroundColor="White" />
        <Button Grid.Row="0" Grid.Column="3" BackgroundColor="White" />
        <Button Grid.Row="1" Grid.Column="0" BackgroundColor="White" />
        <Button Grid.Row="1" Grid.Column="1" BackgroundColor="White" />
        <Button Grid.Row="1" Grid.Column="2" BackgroundColor="White" />
        <Button Grid.Row="1" Grid.Column="3" BackgroundColor="White" />
        <Button Grid.Row="2" Grid.Column="0" BackgroundColor="White" />
        <Button Grid.Row="2" Grid.Column="1" BackgroundColor="White" />
        <Button Grid.Row="2" Grid.Column="2" BackgroundColor="White" />
        <Button Grid.Row="2" Grid.Column="3" BackgroundColor="White" />
        <Button Grid.Row="3" Grid.Column="0" BackgroundColor="White" />
        <Button Grid.Row="3" Grid.Column="1" BackgroundColor="White" />
        <Button Grid.Row="3" Grid.Column="2" BackgroundColor="White" />
        <Button Grid.Row="3" Grid.Column="3" BackgroundColor="White" />
        <Button Grid.Row="4" Grid.Column="0" BackgroundColor="White" />
        <Button Grid.Row="4" Grid.Column="1" BackgroundColor="White" />
        <Button Grid.Row="4" Grid.Column="2" BackgroundColor="White" />
        <Button Grid.Row="4" Grid.Column="3" BackgroundColor="White" />
        <Button Grid.Row="5" Grid.Column="0" BackgroundColor="White" />
        <Button Grid.Row="5" Grid.Column="1" BackgroundColor="White" />
        <Button Grid.Row="5" Grid.Column="2" BackgroundColor="White" />
        <Button Grid.Row="5" Grid.Column="3" BackgroundColor="White" />
        <Button Grid.Row="6" Grid.Column="0" BackgroundColor="White" />
        <Button Grid.Row="6" Grid.Column="1" BackgroundColor="White" />
        <Button Grid.Row="6" Grid.Column="2" BackgroundColor="White" />
        <Button Grid.Row="6" Grid.Column="3" BackgroundColor="White" />
        <Button Grid.Row="7" Grid.Column="0" BackgroundColor="White" />
        <Button Grid.Row="7" Grid.Column="1" BackgroundColor="White" />
        <Button Grid.Row="7" Grid.Column="2" BackgroundColor="White" />
        <Button Grid.Row="7" Grid.Column="3" BackgroundColor="White" />
    </Grid>
    </ScrollView>
</StackLayout>
	</ContentPage.Content>
</ContentPage>
